{
    "name": "dominikb/composer-license-checker",
    "description": "Utility to check for used licenses and analyze them.",
    "keywords": [
        "dominikb",
        "composer-license-checker"
    ],
    "homepage": "https://github.com/dominikb/composer-license-checker",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "dominik",
            "email": "bauernfeind.dominik@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dominikb\\ComposerLicenseChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dominikb\\ComposerLicenseChecker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
